[NGINX_LOG]
#Path for the log file (Nginx)
logpath = /var/log/nginx/access.log

[GEOIP]
#Path for the GEOIP DB file
geoipdb = ./GeoLite2-City.mmdb

[INFLUXDB]
# Database URL
host = ip_address
port = 8086

#Database name
database = db_name

# HTTP Auth
username = "username"
password = "password"

#  Measurement name
measurement = geodata

[GLOBAL]
hostname = hostname